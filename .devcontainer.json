{
  "name": "operations-research-two-tsp",
  "image": "mcr.microsoft.com/devcontainers/base:debian",
  "features": {
    "ghcr.io/msclock/features/vcpkg:2": {
      "username": "vscode",
      "vcpkgversion": "latest"
    },
    "ghcr.io/devcontainers/features/python:1": {
      "pythonVersion": "3.11",
      "installTools": true
    }
  },
  "extensions": [
    "ms-vscode.cmake-tools",
    "ms-vscode.cpptools",
    "ms-python.python",
    "ms-python.vscode-pylance"
  ],
  "remoteUser": "vscode",
}
